  "documentation": "https://docs.langflow.org/components-data#url",
  "icon": "layout-template",
  "minimized": null,
  "tags": [],
  "inputs": [
    {
      "field_class": "MessageTextInput",
      "name": "urls",
      "display_name": "URLs",
